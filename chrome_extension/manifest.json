{
  "name":"Meteor Bookmarks",
  "version" : "0.1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
    },
  "description":"Chrome extension for social bookmarking",
  "browser_action": {
    "default_icon": {
      "19" :"icon_19.png",
      "38" :"icon_38.png"
    },
    "default_title": "Meteor Bookmarks",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
